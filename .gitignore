# Ignore contrib content generated by Composer
/.drush-lock-update
/drush/Commands/contrib/

# Ignore packager folders
/vendor/
node_modules

# Ignore everything by default.
/web/*

# Commit themes and modules
!/web/themes
/web/themes/*
!/web/themes/custom
!/web/modules
/web/modules/*
!/web/modules/custom

# Commit these settings
!/web/sites
/web/sites/*
!/web/sites/default
/web/sites/default/*
!/web/sites/default/settings.php
!/web/sites/default/services.yml
!/web/sites/default/settings.development.php
!/web/sites/default/services.development.yml

# Local only settings
/.env
.lando.local.yml

# OS Annoyances
.DS_Store

# Ignore files generated by editors
/.idea/
/.editorconfig
.dccache

# Danger zone
*.log
*.sql
*.gz

# Custom
/.gitattributes
